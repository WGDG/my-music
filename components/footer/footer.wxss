/* components/footer/footer.wxss */
.index-footer{
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 20rpx;
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 88rpx;
  background: #fff;
}

.popular-image{
  width: 34rpx;
  height: 34rpx;
}
.popular-image image{
  width: 100%;
  height: 100%;
}
.popular{
  width: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}