/* components/movie/play/play.wxss */
.musicButton{
  width: 122rpx;
  height: 122rpx;
  border-radius: 50%;
  background: #fff;
  position: absolute;
  top: 35% ;
  left: 35%;
  padding: 38rpx 40rpx 30rpx 52rpx;
  box-sizing: border-box;
}
.imageMusic{
  width: 30rpx;
  height: 45rpx;
  margin: 0 auto;
}
.musicButton .imageMusic image{
  width: 100%;
  height: 100%;
  border-radius: 0;
}