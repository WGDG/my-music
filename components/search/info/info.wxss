/* components/search/info/info.wxss */
.title{
  padding:30rpx;
  display: flex;
  height:38rpx;
}
.line{
  border:8rpx solid #000;
}
.h{
  font-size: 27rpx;
  padding-left:21rpx;
}
.historyInfo{
  display: flex;
  flex-wrap: wrap;
  padding-left:60rpx;
}
.infolist{
  font-size: 27rpx;
  height:52rpx;
  line-height: 52rpx;
  background: #f7f7f7;
  padding:0rpx 25rpx;
  margin:20rpx 30rpx;
  box-sizing: border-box;
  border-radius: 6rpx;
}
.list{
  width: 100%;
  padding: 0 60rpx;
  box-sizing: border-box;
  border-top: 1px solid #eeeeee;
}
.bookItem{
  width: 100%;
  height: 143rpx;
  border-top: 1px solid #eeeeee;
  display: flex;
  align-items: center;
}
.bookItemImage{
  width: 60rpx;
  height: 80rpx;
}
.bookItemImage image{
  width: 100%;
  height: 100%;
}
.bookItemAt{
  margin-left: 20rpx; 
}
.itemTitle{
  font-size: 26rpx;
}
.author{
  font-size: 18rpx;
  color: #ccc;
  margin-top: 20rpx;
}
