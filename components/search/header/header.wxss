/* components/search/header/header.wxss */
.search-header{
  width:100%;
  height:109rpx;
   
}
.header{
  width:100%;
  height:106rpx;
  display:flex;
}
.input{
  width:539rpx;
  height:69rpx;
  padding-top:10rpx;
  border-radius: 35rpx;
  margin:24rpx 30rpx;
  display:flex;
  justify-content: center;
  background: #f7f7f7;
  position: relative;
}
input{
  font-size: 30rpx;
  line-height: 30rpx;
  border:none;
  outline:none;
}
.detail{
  position: absolute;
  top: 23rpx;
  right: 24rpx;
  width: 26rpx;
  height: 26rpx;
}
.search-banner{
  width:30rpx;
  height:30rpx;
  position:absolute;
  left:29rpx;
  top:21rpx;
}
.cancel{
  width:122rpx;
  height:66rpx;
  border-radius: 26rpx;
  border:1px solid #666666;
  margin-top:23rpx;
  margin-left:10rpx;
}
.can{
  font-size: 26rpx;
  padding-left:32rpx;
}

